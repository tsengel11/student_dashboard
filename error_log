SELECT c.id,
 c.fullname,
 ROUND(SUM(IF(i.itemname = 'Written Activities')||(i.itemname = 'Written Assessment')||(i.itemname = 'Written Activity'),
             g.finalgrade / g.rawgrademax * 100,
             NULL)),
         1) AS 'written',
 ROUND(SUM(IF((i.itemname = 'Summative Activities')||(i.itemname = 'Summative Assessment'),
             g.finalgrade / g.rawgrademax * 100,
             NULL)),
         1) AS 'summative',
 ROUND(SUM(IF(i.itemtype = 'course',
             g.finalgrade / g.rawgrademax * 100,
             NULL)),
         1) AS 'overall'
FROM
 mdl_grade_grades AS g
     LEFT JOIN
 mdl_grade_items AS i ON g.itemid = i.id
     LEFT JOIN
 mdl_course AS c ON i.courseid = c.id
WHERE
 i.courseid IN (
     450,
    451,
    457,
    487,
    452,
    453,
    458,
    547,
    439,
    440,
    441)
 AND userid = ?
GROUP BY i.courseid
